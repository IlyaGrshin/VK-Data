{
  "name": "vkdata",
  "version": "1.0.0",
  "description": "Figma Plugin with a data from VK",
  "main": "code.js",
  "scripts": {
    "build": "/usr/local/bin/node node_modules/.bin/webpack --mode=production",
    "build:watch": "/usr/local/bin/node node_modules/.bin/webpack --mode=development --watch",
    "prettier:format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,json}' "
  },
  "author": "Ilya Grishin",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^14.0.1",
    "preact": "^10.4.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.15.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "compression-webpack-plugin": "^4.0.0",
    "css-loader": "^3.5.3",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "lint-staged": ">=10",
    "prettier": "2.0.5",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.2",
    "tslint": "^6.1.2",
    "tslint-react": "^5.0.0",
    "typescript": "^3.5.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,json}": [
			"prettier --write",
			"git add"
		]
  }
}
